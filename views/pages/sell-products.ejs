<div class="container-fluid" id="sellProductsContainer">
	<div class="wrapper d-none">
		<h1 class="title-medium"><%= UI_DISPLAY_LANG.views.pages.sell_products.sectionTitle %></h1>
	</div>
	<div class="mt-3 app-block">
		<div class="alert alert-info my-2" style="display:none;" id="ERROR_BOX">
			<img src="assets/img/utils/info.png" class="alert-icon" alt="">
			<div class="alert-text" id="text"></div>
		</div>
		<div id="wrapper01">
			<form action="#" id="addForm">
				<div class="row gx-4 gy-4">
					<div class="col-lg-6 col-md-6 col-sm-12">
						<div class="mb-2 section">
							<label for="" class="form-label"><%= UI_DISPLAY_LANG.views.pages.sell_products.form01.label01 %></label>
							<input type="text" class="input-text input-text-outline border-bottom-forced mb-1" id="patientsSearchInput" placeholder="<%= UI_DISPLAY_LANG.views.pages.sell_products.form01.ph01 %>">
							<select name="" id="patientSelect" class="input-text"></select>
						</div>
						<div class="mb-2 section">
							<label for="" class="form-label"><%= UI_DISPLAY_LANG.views.pages.sell_products.form01.label02 %></label>
							<input type="text" class="input-text input-text-outline border-bottom-forced mb-1" id="productsSearchInput" placeholder="<%= UI_DISPLAY_LANG.views.pages.sell_products.form01.ph01 %>">
							<select name="" id="productSelect" class="input-text"></select>
						</div>	
					</div>
					<div class="col-lg-6 col-md-6 col-sm-12">
						<div class="mb-2">
							<label for="" class="form-label"><%= UI_DISPLAY_LANG.views.pages.sell_products.form01.label03 %></label>
							<input type="number" step="any" min="0" class="input-text input-text-outline" id="quantityInput" value="1">
						</div>
						<div class="mb-2 section">
							<label for="" class="form-label"><%= UI_DISPLAY_LANG.views.pages.sell_products.form01.label04 %></label>
							<input type="number" step="any" value="0" class="input-text input-text-outline" id="sellPriceInput">
						</div>
						<div class="mb-2">
							<label for="" class="form-label"><%= UI_DISPLAY_LANG.views.pages.sell_products.form01.label06 %></label>
							<input type="number" readonly step="any" value="0" class="input-text input-text-outline" id="finalAmountInput">
						</div>	
					</div>
					<div class="col-lg-12 col-md-12 col-sm-12">
						<div class="mb-2">
							<label for="" class="form-label"><%= UI_DISPLAY_LANG.views.pages.sell_products.form01.label05 %></label>
							<textarea rows="4" class="input-text input-text-outline" id="noteInput"></textarea>
						</div>		
					</div>
				</div>
				<div class="">
					<input type="submit" class="btn btn-primary btn-sm shadow" value="<%= UI_DISPLAY_LANG.views.pages.sell_products.form01.submitBTN01 %>">
				</div>
			</form>	
		</div>
	</div>
</div>