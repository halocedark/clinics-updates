{
  "name": "Clinics",
  "version": "0.1.0",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/halocedark/clinics-updates.git"
  },
  "build": {
    "asar": true,
    "afterPack": "afterPack.js",
    "publish": [
      {
        "provider": "github",
        "owner": "halocedark",
        "repo": "clinics-updates"
      }
    ],
    "appId": "com.holoolaz.clinics",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/ico/favicon.ico",
      "requestedExecutionLevel": "highestAvailable"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "start": "electron .",
    "publish": "electron-builder -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "asarmor": "^2.0.0",
    "ejs-electron": "^2.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-root-path": "^1.0.16",
    "electron-updater": "^4.6.5",
    "express": "^4.17.1",
    "ini": "^2.0.0",
    "jquery": "^3.6.0",
    "papaparse": "^5.3.2",
    "qrcode": "^1.5.0",
    "read-excel-file": "^5.2.28",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "electron": "^16.0.9",
    "electron-builder": "^22.14.13"
  }
}
